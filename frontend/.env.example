# React App Configuration
REACT_APP_ENV=development
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_TIMEOUT=10000

# Blockchain Configuration
REACT_APP_ETHEREUM_RPC_URL=http://localhost:8545
REACT_APP_ETHEREUM_RPC_URL_SEPOLIA=https://sepolia.infura.io/v3/your_infura_project_id
REACT_APP_ETHEREUM_RPC_URL_MAINNET=https://mainnet.infura.io/v3/your_infura_project_id
REACT_APP_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
REACT_APP_CHAIN_ID=31337
REACT_APP_CHAIN_ID_SEPOLIA=11155111
REACT_APP_CHAIN_ID_MAINNET=1

# Network Configuration
REACT_APP_DEFAULT_NETWORK=localhost
REACT_APP_SUPPORTED_NETWORKS=localhost,sepolia,mainnet

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_MOCK_DATA=false

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=pdf,doc,docx,jpg,jpeg,png

# UI Configuration
REACT_APP_THEME=light
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_TIMEZONE=UTC

# External Services
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Development
GENERATE_SOURCEMAP=true
FAST_REFRESH=true