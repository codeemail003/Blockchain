# Example .env file for backend
NODE_ENV=development
PORT=3000
HOST=localhost

# Database
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_JWT_SECRET=

# AWS
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=

# JWT
JWT_SECRET=
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Blockchain
ETHEREUM_RPC_URL=
ETHEREUM_RPC_URL_SEPOLIA=
ETHEREUM_RPC_URL_MAINNET=
PRIVATE_KEY=
CONTRACT_ADDRESS=

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,doc,docx,jpg,jpeg,png
UPLOAD_PATH=./uploads

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# CORS
CORS_ORIGIN=http://localhost:3001
CORS_CREDENTIALS=true

# Health Check
HEALTH_CHECK_INTERVAL=30000

# Security
BCRYPT_ROUNDS=12
SESSION_SECRET=

# Email (Optional)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
FROM_EMAIL=

# Monitoring (Optional)
SENTRY_DSN=
NEW_RELIC_LICENSE_KEY=

# Cache (Optional)
REDIS_URL=
REDIS_PASSWORD=

# API Keys (Optional)
INFURA_PROJECT_ID=
ALCHEMY_API_KEY=
ETHERSCAN_API_KEY=
