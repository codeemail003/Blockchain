NODE_ENV=production
PORT=3000
CORS_ORIGINS=https://app.example.com,https://admin.example.com
JSON_BODY_LIMIT=1mb
RATE_WINDOW_MS=60000
RATE_MAX=120
HTTPS_ENABLED=true
HTTPS_KEY_PATH=/etc/ssl/private/pharbitchain.key
HTTPS_CERT_PATH=/etc/ssl/certs/pharbitchain.crt
DB_PATH=/var/lib/pharbitchain/blockchain-db
DIFFICULTY=4
MINING_REWARD=50
BLOCK_SIZE=1000
NETWORK_ID=pharbitchain-mainnet
MAX_PEERS=100
JWT_SECRET=please-set-in-secrets-manager

